{
    "name": "tomahawk/framework",
    "description": "The Tomahawk PHP Framework.",
    "keywords": ["framework", "tomahawk"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Ellis",
            "email": "tellishtc@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin",
        "process-timeout": 10000,
        "github-protocols": ["https"]
    },
    "require": {
        "php": ">=5.3.0",
        "illuminate/database": "4.1.*",
        "classpreloader/classpreloader": "1.0.*",
        "doctrine/dbal": "2.4.*",
        "doctrine/cache": "1.*",
        "ircmaxell/password-compat": "1.0.*",
        "filp/whoops": "1.0.7",
        "jeremeamia/superclosure": "1.0.*",
        "monolog/monolog": "1.*",
        "nesbot/carbon": "1.*",
        "patchwork/utf8": "1.1.*",
        "swiftmailer/swiftmailer": "5.2.*",
        "symfony/browser-kit": "2.5.*",
        "symfony/console": "2.5.*",
        "symfony/config": "2.5.*",
        "symfony/css-selector": "2.5.*",
        "symfony/debug": "2.5.*",
        "symfony/dom-crawler": "2.5.*",
        "symfony/event-dispatcher": "2.5.*",
        "symfony/finder": "2.5.*",
        "symfony/http-foundation": "2.5.*",
        "symfony/http-kernel": "2.5.*",
        "symfony/process": "2.5.*",
        "symfony/routing": "2.5.*",
        "symfony/security-core": "2.5.*",
        "symfony/translation": "2.5.*",
        "symfony/templating": "2.5.*",
        "twig/twig": "1.*",
        "pimple/pimple": "2.*",
        "phpseclib/phpseclib" : "0.3",
        "predis/predis" : "1.0.0"
    },
    "require-dev": {
        "mockery/mockery": "0.8.0",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "classmap": [

        ],
        "files": [

        ],
        "psr-4": {
            "Tomahawk\\": "src/Tomahawk/"
        }
    },
    "minimum-stability": "stable"
}
